<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.xj.hookdemo"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".TestOnClickActivity">
        </activity>
        <activity android:name=".activityhook.TestHookStartActivity">
        </activity>
        <activity android:name=".TestNotificationActivity">
        </activity>
        <activity android:name=".activityhook.TestActivityStart">
        </activity>
        <activity android:name=".activityhook.TestApplicationStart">
        </activity>
        <activity android:name=".activityhook.TestIActivityManagerStart">
        </activity>
        <activity android:name=".activityhook.TestClipboardActivity">
        </activity>
        <activity android:name=".activityhook.EmptyActivity">
        </activity>
        <activity android:name=".activityhook.TestStartActivityNoRegister">
        </activity>
    </application>

</manifest>